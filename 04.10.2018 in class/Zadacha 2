using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace ConsoleApplication1
{
    class Program
    {
        static void Main(string[] args)
        {
            double cm = double.Parse(Console.ReadLine());
            Console.Write(cm);
            Console.Write("cm = ");
            Console.Write(2.54*cm);
            Console.Write("inc");
            Console.ReadLine();
        }
    }
}
